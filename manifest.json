{
    "name": "My first extension",
    "description": "Base Level Extension",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_popup": "hello.html",
      "default_icon": "chess.png"
    },

    "content_scripts": [
      {
        "matches": ["https://www.chess.com/play/*"],
        "js": ["content.js"]
      }
    ]
  }